package com.devsuperiorT.dscommerce.dto;

import com.devsuperiorT.dscommerce.entities.User;

public class ClientDTO {
	private Long id;
	private String name;
	
	public ClientDTO(Long id, String name) {
		this.id = id;
		this.name = name;
	}
	
	public ClientDTO(User entity) {
		id = entity.getId();
		name = entity.getName();
	}

}
